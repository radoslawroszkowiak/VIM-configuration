{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "jquery": true,
        "jasmine": true,
        "mocha": true,
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    
    },
    "plugins": [
        "react"
    
    ],
    "rules": {
        "strict": 0,
        "quotes": 0,
        "indent": [2, 4],
        "no-unused-vars": 0,
        "camelcase": 0,
        "no-underscore-dangle": 0
    
    },
    "extends": "airbnb/legacy"
}
